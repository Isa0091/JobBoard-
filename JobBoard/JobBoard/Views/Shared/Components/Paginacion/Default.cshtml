@model JobBoard.Models.Paging.PagingVm


<div class="btn-group">
    @if (Model.UrlPaginaAnterior != null)
    {
        <a class="btn-white btn @(Model.ClasePersonalizada)" href="@Model.UrlPaginaAnterior" aria-label="Previous"> <span aria-hidden="true">&laquo;</span> <span class="sr-only">Previous</span> </a>
    }
    @foreach (var item in Model.Botones)
    {
        <a class="btn-white btn  @(Model.ClasePersonalizada) @if(item.Pagina == Model.PaginaActiva){@("active")}" href="@item.UrlBoton">@item.Pagina</a>
    }
    @if (Model.UrlPaginaSiguiente != null)
    {
        <a class="btn-white btn  @(Model.ClasePersonalizada)" href="@Model.UrlPaginaSiguiente" aria-label="Next"> <span aria-hidden="true">&raquo;</span> <span class="sr-only">Next</span> </a>
    }

</div>
